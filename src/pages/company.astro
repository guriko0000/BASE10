<Layout title="COMPANY">
  <section class="p-under-mv">
    <div class="l-inner">
      <h1 class="p-under-mv__title">
        <span class="en">Company</span>
        <span class="ja">会社案内</span>
      </h1>
    </div>
  </section>
  <section class="l-section l-section--first">
    <div class="l-inner">
      <h2 class="c-color-title">会社概要</h2>
  </section>



  <section class="p-service l-section l-section--last">
    <div class="l-inner">
      <h2 class="c-color-title">サービスの流れ</h2>

    </div>
  </section>
	
</Layout>









